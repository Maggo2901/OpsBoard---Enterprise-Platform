# Backend Port (internal to container network usually, but good to set)
PORT=3000

# Environment Mode
NODE_ENV=production

# Database Path in Container
DB_PATH=/data/database.sqlite

# Uploads Path in Container
UPLOADS_PATH=/app/data/uploads

# CORS Origin
# Set this to http://<VM-IP>:8080 or the domain you use
CORS_ORIGIN=http://192.168.1.100:8080
